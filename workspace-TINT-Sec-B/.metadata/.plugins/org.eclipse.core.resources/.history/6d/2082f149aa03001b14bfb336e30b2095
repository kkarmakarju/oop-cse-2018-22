package in.edu.tint.cse.demo;

public class TestMain {

	public static void main(String[] args) {
//		MyThread myThread = new MyThread("MyThread");
//		for (int x = 1; x < 50; x++) {
//			System.out.println("Hello");
//		}

		//fork
		
		MyThreadRunnable myThread1 = new MyThreadRunnable("MyThread2", 1);
		MyThreadRunnable myThread2 = new MyThreadRunnable("MyThread2", 2);
		MyThreadRunnable myThread3 = new MyThreadRunnable("MyThread2", 3);

		try {
			myThread1.t.join();
			myThread2.t.join();
			myThread3.t.join();			
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		
		System.out.println("TestMain");
	}
}
